    "import numpy as np\n",
    "\n",
    "# index\n",
    "n1 = np.random.randint(1, 10, [3, 3])\n",
    "print(n1)\n",
    "# 获取第一列\n",
    "print(n1[0])\n",
    "# 获取第一列中的第二个值\n",
    "print(n1[0][1])\n",
    "print(n1[0, 1])\n",
    "\n",
    "# slice\n",
    "# 获取前两列\n",
    "print(n1[0:2])\n",
    "# start stop step\n",
    "print(n1[0:2:2])"
   ]
